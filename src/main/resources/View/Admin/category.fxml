<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.categoryController">
    <BorderPane.margin>
        <Insets bottom="10.0" left="10.0" right="20.0" top="10.0" />
    </BorderPane.margin>
    <center>
        <VBox spacing="10.0">
            <HBox alignment="CENTER" prefHeight="47.0" spacing="100.0">
                <children>
                    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                        <children>
                            <Button fx:id="selectAll" mnemonicParsing="false" text="Select All" />
                        </children>
                    </HBox>
                    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                        <children>
                            <Button fx:id="add" mnemonicParsing="false" text="Add" />
                        </children>
                    </HBox>
                    <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                        <children>
                            <Button fx:id="reload" mnemonicParsing="false" text="Reload" />
                        </children>
                    </HBox>
                </children>
            </HBox>
            <TableView fx:id="categoryTable" prefWidth="975.0">
                <columns>
                    <TableColumn fx:id="selectColumn" prefWidth="129.0" text="Select" />
                    <TableColumn fx:id="idColumn" prefWidth="234.0" text="ID" />
                    <TableColumn fx:id="categoryNameColumn" prefWidth="187.0" text="Category Name" />
                    <TableColumn fx:id="created_timeColumn" prefWidth="187.0" text="Created time" />
                    <TableColumn fx:id="actionColumn" prefWidth="130.0" text="Action" />
                </columns>
            </TableView>
        </VBox>
    </center>
</BorderPane>

